{
  "name": "sampo02-blog",
  "version": "1.0.0",
  "private": true,
  "description": "sampo02-bog",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "gatsby develop --host 0.0.0.0",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint:check": "yarn run prettier -c . && yarn run eslint --max-warnings 0 . && yarn run textlint src/markdown-pages/",
    "lint:fix": "yarn run prettier --write . && yarn run eslint '**/*.{ts,tsx}' --quiet --fix && yarn run textlint --fix src/markdown-pages/",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "gatsby": "^3.8.1",
    "gatsby-plugin-react-helmet": "^4.9.0",
    "gatsby-plugin-robots-txt": "^1.6.8",
    "gatsby-plugin-sass": "^4.9.0",
    "gatsby-plugin-sitemap": "^4.4.0",
    "gatsby-remark-prismjs": "^5.6.0",
    "gatsby-source-filesystem": "^3.9.0",
    "gatsby-transformer-remark": "^4.5.0",
    "prismjs": "^1.24.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "sass": "^1.35.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.21.6",
    "@graphql-codegen/typescript": "1.22.4",
    "@types/node": "^16.0.0",
    "@types/react": "^17.0.13",
    "@types/react-dom": "^17.0.8",
    "@types/react-helmet": "^6.1.1",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint": "^7.29.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.24.0",
    "gatsby-plugin-prettier-eslint": "^1.0.6",
    "gatsby-plugin-ts-config": "^1.1.5",
    "prettier": "^2.3.2",
    "textlint": "^12.0.0",
    "textlint-rule-preset-japanese": "^7.0.0",
    "typescript": "^4.3.5"
  }
}
